    td:specificationReference <https://solidproject.org/TR/protocol#spec2> ;
    td:preCondition "sf1" ;
    dcterms:hasPart
        <https://example.org/group2/feature1>
.

<https://example.org/group2/feature1>
    a td:TestCase ;
    dcterms:subject "MUST" ;
    td:reviewStatus td:accepted ;
    dcterms:title "Group 2 Test case 1" .

manifest:group3
