{
    "testsuite": "http://solidcommunity.org/testsuite#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "dc": "http://purl.org/dc/elements/1.1/",
    "dct": "http://purl.org/dc/terms/",
    "dctype": "http://purl.org/dc/dcmitype/",
    "earl": "http://www.w3.org/ns/earl#",

    "Feature": "earl:TestCase",
    "Background": "testsuite:Background",
    "Scenario": "testsuite:Scenario",
    "*": "testsuite:Def",
    "Given": "testsuite:Given",
    "And": "testsuite:And",
    "When": "testsuite:When",
    "Then": "testsuite:Then",

    "uri": "@id",
    "name": "rdfs:label",
    "description": "rdfs:comment",
    "keyword": "testsuite:keyword",

    "elements": {
        "@id": "earl:assertions",
        "@container": "@set"
    },
    "steps": {
        "@id": "earl:result",
        "@container": "@set"
    },
    "result": "testsuite:hasResult",
    "status": {
        "@id": "earl:outcome",
        "@type": "@id"
    },
    "doc_string": "testsuite:trace",
    "value": "earl:info"
}
